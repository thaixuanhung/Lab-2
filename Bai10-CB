#include <iostream>
using namespace std;
int main()
{
    float diemT;
    float diemV;
    float diemA;
    float dtb;

    cout<<"Nhap diem toan: ";
    cin>>diemT;
    cout<<"Nhap diem van: ";
    cin>>diemV;
    cout<<"Nhap diem anh: ";
    cin>>diemA;

    dtb = (diemT + diemV + diemA) / 3;
    cout<<"Diem trung binh la: "<<dtb;
    
    if (dtb < 3.0){
    	cout<<" KEM";
    }else if ((3.0 <= dtb)&&(dtb < 5.0)){
        cout<<" YEU";
    }else if ((5.0 <= dtb)&&(dtb < 6.5)){
        cout<<" TB";
    }else if ((6.5 <= dtb)&&(dtb < 8.0)){
        cout<<" KHA";
    }else if ((8.0 <= dtb)&&(dtb < 9.0)){
        cout<<" GIOI";
    }else{
    	cout<<" XUAT SAC";
	}
	return 0;
}
